{
  "$schema": "http://json-schema.org/draft-04/schema#",

    "type": "object",
    "properties": {
        "id": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "created": {
            "type": "string",
            "format": "date-time"
        },
        "updated": {
          "type": "string",
          "format": "date-time"
        },
        "_embedded": {
            "type": "object",
            "javaName": "WorldEmbedded",
            "properties": {
                "owner": {
                    "$ref": "userBrief.json"
                }
            },
            "required": [
                "owner"
            ]
        }
    },
    "required": [
        "id",
        "name",
        "created",
        "updated"
    ]
}
